    agent any

    stages {
        stage('Build') {
            steps {
                echo 'this is my build stage'
            }
        }

        stage('Test') {
            steps {
                echo 'this is my test stage'
            }            
        }

        stage('Release') {
            steps {
                echo 'this is my release stage'
            }
        }
    }
} 